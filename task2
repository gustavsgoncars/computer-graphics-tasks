   "source": [
    " import numpy as np               \n",
    "import matplotlib.pyplot as plt  \n",
    "\n",
    "img = np.ones((600,600,3))\n",
    "\n",
    "#Man ir 3 kontrolpunkti \n",
    "                #P0    P1      P2     \n",
    "def DrawBezier(x0,y0, x1,y1, x2,y2, x3,y3, x4,y4, x5,y5):\n",
    "    \n",
    "    x = x0 # sākumpunkta definēšana pēc x ass\n",
    "    y = y0 # sākumpunkta definēšana pēc y ass\n",
    "    t = 0 # parametrs t, kas pieder intervālam no 0 līdz 1\n",
    "    ts = 0.001 # solis, ar kuru mainās parametrs t. Jo mazāks ir solis ts,\n",
    "    #jo vairāk punktu uz līknes uzzīmēs algoritms. \n",
    "\n",
    "    while t <= 1.0:  # kamēr nav sasniegts interavala gala punkts\n",
    "        \n",
    "        B0 = 1*(1-t)**5 #rēķinam Beršteina 0.polinomu\n",
    "        B1 = 5*(1-t)**4*t#rēķinam Beršteina 1.polinomu\n",
    "        B2 = 10*(1-t)**3*t**2#rēķinam Beršteina 2.polinomu\n",
    "        B3 = 10*(1-t)**2*t**3\n",
    "        B4 = 5*(1-t)*t**4\n",
    "        B5 = 1*t**5\n",
    "       \n",
    "        x = int(x0*B0 + x1*B1 + x2*B2 + x3*B3 + x4*B4 + x5*B5) #rēķinam līknes pikseļa koordinātes pēc x ass\n",
    "        y = int(y0*B0 + y1*B1 + y2*B2 + y3*B3 + y4*B4 + y5*B5) #rēķinam līknes pikseļa koordinātes pēc y ass\n",
    "        \n",
    "        img[y,x] = (1,0,0) #zīmējam tekošo pikseli sarkanajā krāsā\n",
    "        \n",
    "        t=t+ts #palielinam t vērtību\n",
    "        \n",
    "    return\n",
    "             # P0         P1          P2          \n",
    "DrawBezier(350,150,   100,300,   200,300,   200,400,    300,500,    400,400)\n",
    "DrawBezier(400,400,   400,300,   400,250,   350,250,    300,250,    190,300)\n",
    "    \n",
    "%matplotlib notebook\n",
    "plt.figure(figsize=(10,8), dpi = 100, facecolor='w')\n",
    "plt.imshow(img)\n",
    "plt.show()"
   ]
  },
